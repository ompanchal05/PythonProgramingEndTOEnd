   "source": [
    "### slicing in array using numpy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "cad51f9b",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "42a92006",
   "metadata": {},
   "outputs": [],
   "source": [
    "a = np.array([10,20,30,40,50])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "277366b2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[20 30 40]\n",
      "[10 20 30]\n",
      "[40 50]\n",
      "[10 20 30 40]\n",
      "[30]\n"
     ]
    }
   ],
   "source": [
    "#Slice from index 1 to 4\n",
    "\n",
    "print (a[1:4])\n",
    "print(a [:3])\n",
    "print(a [3:])\n",
    "print(a [:-1])\n",
    "print(a [2:3])"
   ]
  }
